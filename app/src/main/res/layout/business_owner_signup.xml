<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/page_background_theme_neutralizer"
    tools:context=".BusinessOwnerSignupActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/textView11"
                android:layout_width="317dp"
                android:layout_height="37dp"
                android:text="Bby Clicking Register, you agree to our Terms, Data Policy and Cookie Policy. You May Recieve SMS notification from us and can opt out any time"
                android:textAlignment="center"
                android:textSize="9dp"
                app:layout_constraintBottom_toTopOf="@+id/businessOwner_SignupButton"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessReEnterPassword_ET"
                app:layout_constraintHorizontal_bias="0.487"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessReEnterPassword_ET" />

            <Button
                android:id="@+id/businessOwner_SignupButton"
                android:layout_width="346dp"
                android:layout_height="53dp"
                android:layout_marginTop="1100dp"
                android:backgroundTint="#f54242"
                android:text="Register"
                android:textColor="#fff"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.492"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.915" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessName_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Business Name"
                android:inputType="textPersonName"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView10" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessAddress_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Business Address"
                android:inputType="textPersonName"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessName_ET"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessName_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessName_ET" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessHotLine_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Hotline/Phone Number"
                android:inputType="textPersonName"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessAddress_ET"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessAddress_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessAddress_ET" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessUsername_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Username
"
                android:inputType="textPersonName"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessHotLine_ET"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessHotLine_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessHotLine_ET" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessEmail_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Email"
                android:inputType="textPersonName"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessUsername_ET"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessUsername_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessUsername_ET" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessPassword_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Password"
                android:inputType="textPassword"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessEmail_ET"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessEmail_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessEmail_ET" />

            <EditText
                android:id="@+id/businessOwnerSignup_BusinessReEnterPassword_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Re-Enter Password"
                android:inputType="textPassword"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessPassword_ET"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessPassword_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessPassword_ET" />

            <EditText
                android:id="@+id/businessOwnerSignup_DTINo_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Please Enter DTI No."
                android:inputType="text"
                android:paddingLeft="5dp"
                app:layout_constraintEnd_toEndOf="@+id/businessOwnerSignup_BusinessPassword_ET"
                app:layout_constraintStart_toStartOf="@+id/textView69"
                app:layout_constraintTop_toBottomOf="@+id/textView69" />

            <EditText
                android:id="@+id/businessOwnerSignup_MayorPermitNo_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Please Enter Mayor's Permit No."
                android:inputType="text"
                android:paddingLeft="5dp"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_DTINo_ET"
                app:layout_constraintTop_toBottomOf="@+id/textView73" />

            <EditText
                android:id="@+id/businessOwnerSignup_BIRRegNo_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Please Enter BIR Registered No."
                android:inputType="text"
                android:paddingLeft="5dp"
                app:layout_constraintStart_toStartOf="@+id/textView74"
                app:layout_constraintTop_toBottomOf="@+id/textView74" />

            <EditText
                android:id="@+id/businessOwnerSignup_PhilGepsRegNo_ET"
                android:layout_width="356dp"
                android:layout_height="49dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/input_styles"
                android:ems="10"
                android:hint="Please Enter BIR Registered No."
                android:inputType="text"
                android:paddingLeft="5dp"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BIRRegNo_ET"
                app:layout_constraintTop_toBottomOf="@+id/textView129" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="244dp"
                android:layout_height="88dp"
                android:layout_marginTop="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/logo" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Business Owner Signup"
                android:textColor="#000"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/imageView6"
                app:layout_constraintStart_toStartOf="@+id/imageView6"
                app:layout_constraintTop_toBottomOf="@+id/imageView6" />

            <TextView
                android:id="@+id/textView69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="DTI # : "
                android:textColor="#000"
                app:layout_constraintStart_toStartOf="@+id/textView72"
                app:layout_constraintTop_toBottomOf="@+id/textView72" />

            <TextView
                android:id="@+id/textView73"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Mayors Permit No."
                android:textColor="#000"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_DTINo_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_DTINo_ET" />

            <TextView
                android:id="@+id/textView74"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="BIR Permit No. : "
                android:textColor="#000"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_MayorPermitNo_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_MayorPermitNo_ET" />

            <TextView
                android:id="@+id/textView129"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="PHILGEPS REGISTERED NO. (Optional)"
                android:textColor="#000"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BIRRegNo_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BIRRegNo_ET" />

            <TextView
                android:id="@+id/textView72"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Please Attach Your Document No. "
                android:textColor="#000"
                app:layout_constraintStart_toStartOf="@+id/businessOwnerSignup_BusinessReEnterPassword_ET"
                app:layout_constraintTop_toBottomOf="@+id/businessOwnerSignup_BusinessReEnterPassword_ET" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>