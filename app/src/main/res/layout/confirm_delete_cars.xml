<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConfirmDeleteCars">

    <TextView
        android:id="@+id/confirmDeleteCars_ID_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Car ID :"
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/imageView21"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/confirmDeleteCars_carName_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Car Name: "
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/confirmDeleteCars_ID_Holder"
        app:layout_constraintTop_toBottomOf="@+id/confirmDeleteCars_ID_Holder" />

    <TextView
        android:id="@+id/confirmDeleteCars_carPice_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Car Price :"
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/confirmDeleteCars_carName_Holder"
        app:layout_constraintTop_toBottomOf="@+id/confirmDeleteCars_carName_Holder" />

    <TextView
        android:id="@+id/confirmDeleteCars_carCompanyOwner_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Car Company Owner :"
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/confirmDeleteCars_carPice_Holder"
        app:layout_constraintTop_toBottomOf="@+id/confirmDeleteCars_carPice_Holder" />

    <TextView
        android:id="@+id/confirmDeleteCars_carCapacity_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Car Capacity : "
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/confirmDeleteCars_carCompanyOwner_Holder"
        app:layout_constraintTop_toBottomOf="@+id/confirmDeleteCars_carCompanyOwner_Holder" />

    <TextView
        android:id="@+id/confirmDeleteCars_carEngine_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Car Engine : "
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/confirmDeleteCars_carCapacity_Holder"
        app:layout_constraintTop_toBottomOf="@+id/confirmDeleteCars_carCapacity_Holder" />

    <TextView
        android:id="@+id/confirmDeleteCars_carDoors_Holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Car Doors : "
        android:textColor="#000"
        android:textSize="15dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/confirmDeleteCars_carEngine_Holder"
        app:layout_constraintTop_toBottomOf="@+id/confirmDeleteCars_carEngine_Holder" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="116dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_name" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Confrim Delete Cars"
        android:textColor="#000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/confirmDelete_confirmDeleteButton"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="400dp"
        android:backgroundTint="@color/primary_color"
        android:text="Confirm Delete"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/confirmDelete_cancelButton"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="30dp"
        android:backgroundTint="@color/primary_color"
        android:text="Cancel"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>